dofile('config.lua')
package.path = string.format('%s/?.lua;', Config.engine_path)..package.path
package.cpath = string.format('%s/lib/?.so;', Config.engine_path)..package.cpath
--导入必须的c库
require('stringpatch')
require('bitpatch')
require('system')
require('json')
require('log')
require('ae')
require('ar')
require('redis')
require('mysql')
require('socket')
